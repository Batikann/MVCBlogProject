@model EntityLayer.Concrete.Admin
@{
    ViewBag.Title = "AdminAuthorProfile";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}




<!DOCTYPE html>

<html>



<body class="hold-transition sidebar-mini">

    <div class="wrapper">

        <section class="content">

            <div class="container-fluid">

                <div class="row">

                    @Html.Action("ProfilePartial","AdminProfile")

                    <div class="col-md-9">
                        <div class="card">

                            <div class="card-header p-2">

                                <ul class="nav nav-pills">

                                    <li class="nav-item"><a class="nav-link" href="#timeline" data-toggle="tab">Aktiviteler</a></li>

                                    <li class="nav-item"><a class="nav-link" href="#settings" data-toggle="tab">Profil Ayarları</a></li>

                                </ul>

                            </div><!-- /.card-header -->

                            <div class="card-body">

                                <div class="tab-content">



                                    <!-- /.tab-pane -->

                                    <div class="tab-pane" id="timeline">

                                        <!-- The timeline -->

                                        @Html.Action("TimelinePartial","AdminProfile")

                                    </div>

                                    <!-- /.tab-pane -->

                                    @Html.Action("Partial1", "AdminProfile")

                                    <!-- /.tab-pane -->

                                </div>

                                <!-- /.tab-content -->

                            </div><!-- /.card-body -->

                        </div>

                        <!-- /.nav-tabs-custom -->

                    </div>

                    <!-- /.col -->

                </div>

                <!-- /.row -->

            </div><!-- /.container-fluid -->

        </section>

        <!-- /.content -->

    </div>



</body>

</html>



